      <property name="styleSheet">
       <string notr="true">/* =========================
   QTabWidget – Base
   ========================= */
QTabWidget::pane {
    border: 1px solid #142a5c;
    background: #f5f8fc;
    top: -1px;
}

/* =========================
   QTabBar – Base Tabs
   ========================= */
QTabBar::tab {
    background: qlineargradient(
        x1:0, y1:0, x2:0, y2:1,
        stop:0 #2a4da3,
        stop:1 #1f3c88
    );

    color: white;
    font-family: &quot;Segoe UI&quot;;
    font-size: 11px;
    font-weight: 600;

    padding: 10px 20px;
    margin-right: 4px;

    border: 0px solid #142a5c;
    border-bottom: none;

    border-top-left-radius: 8px;
    border-top-right-radius: 8px;

    min-width: 120px;
}

/* =========================
   Hover (Soft Cyan Accent)
   ========================= */
QTabBar::tab:hover {
    background: qlineargradient(
        x1:0, y1:0, x2:0, y2:1,
        stop:0 #4fc3dc,
        stop:1 #1f3c88
    );
}

/* =========================
   Selected Tab (Same as Hover)
   ========================= */
QTabBar::tab:selected {
    background: qlineargradient(
        x1:0, y1:0, x2:0, y2:1,
        stop:0 #4fc3dc,
        stop:1 #1f3c88
    );

    color: white;
    border: 0px solid #142a5c;
    border-bottom: 2px solid #4fc3dc;
}

/* Lift selected tab slightly (3D feel) */
QTabBar::tab:selected {
    margin-top: -2px;
}

/* =========================
   Disabled Tab
   ========================= */
QTabBar::tab:disabled {
    background: #bfcbe6;
    color: #6b7aa8;
}
